            },
            "drupal/views_infinite_scroll": {
                "https://www.drupal.org/project/views_infinite_scroll/issues/3094488": "https://www.drupal.org/files/issues/2020-04-14/3094488-2.patch",
                "D8CORE-2949: https://www.drupal.org/project/views_infinite_scroll/issues/3173923": "https://www.drupal.org/files/issues/2021-06-30/views_infinite_scroll-3173923-4.patch"
            },
            "drupal/views_taxonomy_term_name_depth": {
                "https://www.drupal.org/project/views_taxonomy_term_name_depth/issues/2877249": "patches/contrib/views_taxonomy_term_name_depth-mr-2.patch"
            },
            "drupal/webp": {
                "https://www.drupal.org/project/webp/issues/3281606": "patches/contrib/webp-mr-33.patch"
            }
        }
    },
    "scripts": {
        "post-drupal-scaffold-cmd": [
            "patch -Ns -r \"\" --no-backup-if-mismatch -p1 < ./patches/htaccess.patch",
            "patch -Ns -r \"\" --no-backup-if-mismatch -p1 < ./patches/robotstxt.patch"
        ],
        "init-stack": [
            "vendor/bin/blt blt:telemetry:disable --no-interaction",
            "vendor/bin/blt blt:init:settings",
            "vendor/bin/blt sws:keys",
            "vendor/bin/blt recipes:aliases:init:acquia"
        ],
        "sync-sustainability": [
            "vendor/bin/blt drupal:sync:files --site=sustainability",
            "vendor/bin/blt drupal:sync --site=sustainability"
        ]
    },
    "autoload": {
        "psr-4": {
            "Gryphon\\": "blt/src/",
            "Drupal\\sdss_profile\\": "docroot/profiles/sdss/sdss_profile/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\sdss_profile\\": "docroot/profiles/sdss/sdss_profile/tests/src"
        }
    }
}
