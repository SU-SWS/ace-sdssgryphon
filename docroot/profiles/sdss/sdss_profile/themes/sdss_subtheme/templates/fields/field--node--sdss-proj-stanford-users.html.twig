{# templates/fields/field--node--sdss-proj-stanford-users.html.twig #}
{% if items is not empty %}
  <p class="su-project-team">
    <strong>Team:</strong> 
    {% for item in items %}
      {# Accessing the title and url from the entity reference #}
      {% if not item.content['#url'].toString() %}<span>{% else %}<a href="{{ item.content['#url'] }}" class="ext su-link--external" title="(opens in a new window)">{% endif %}{{ item.content['#title'] }}{% if not item.content['#url'].toString() %}</span>{% else %}</a>{% endif %}{% if not loop.last %}, {% endif %}
    {% endfor %}
  </p>
{% endif %}
